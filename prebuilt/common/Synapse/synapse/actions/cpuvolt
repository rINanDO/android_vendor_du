cpuread() {
	while read FREQ UVOLT UNIT; do
		if [ $FREQ == $1 ]; then
			echo $UVOLT
			return
		fi
	done < $P
}

cpuwrite() {
	VOLTARR=""
	while read FREQ UVOLT UNIT; do
		if [ $FREQ == $1 ]; then
			VOLTARR="$VOLTARR $2"
		else
			VOLTARR="$VOLTARR $UVOLT"
		fi
	done < $P

	echo $VOLTARR > $P
}

case $(basename $0) in
cpuvolt)
	P=/sys/devices/system/cpu/cpu0/cpufreq/UV_mV_table
	if [[ ! -z $2 ]]; then
		cpuwrite $1 $2
	fi
	echo $(cpuread $1)
	;;

esac
